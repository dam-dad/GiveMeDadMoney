<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<BorderPane fx:id="settingsView" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17">
   <center>
      <HBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <Label text="Nombre:" />
            <TextField fx:id="userNameText" />
            <Button fx:id="changeUserNameButton" mnemonicParsing="false" onAction="#onChangeUserNameAction" text="Cambiar" />
         </children>
      </HBox>
   </center>
</BorderPane>
