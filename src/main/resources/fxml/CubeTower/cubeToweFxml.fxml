<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane fx:id="view" maxHeight="1.7976931348623157E308"
	maxWidth="1.7976931348623157E308" minHeight="-Infinity"
	minWidth="-Infinity" styleClass="root"
	xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
	<center>
		<GridPane fx:id="pixeles" hgap="5.0" styleClass="juego"
			vgap="5.0" BorderPane.alignment="CENTER">
			<columnConstraints>
				<ColumnConstraints hgrow="ALWAYS" minWidth="10.0"
					prefWidth="100.0" />
				<ColumnConstraints hgrow="ALWAYS" minWidth="10.0"
					prefWidth="100.0" />
				<ColumnConstraints hgrow="ALWAYS" minWidth="10.0"
					prefWidth="100.0" />
				<ColumnConstraints hgrow="ALWAYS" minWidth="10.0"
					prefWidth="100.0" />
				<ColumnConstraints hgrow="ALWAYS" minWidth="10.0"
					prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="ALWAYS" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="ALWAYS" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="ALWAYS" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="ALWAYS" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="ALWAYS" />
			</rowConstraints>
		</GridPane>
	</center>
	<top>
		<HBox alignment="TOP_CENTER" styleClass="bordes"
			BorderPane.alignment="TOP_CENTER">
			<children>
				<Button mnemonicParsing="false" onAction="#onAtrasButton"
					prefHeight="35.0" prefWidth="94.0" text="Volver">
					<HBox.margin>
						<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
					</HBox.margin>
				</Button>
				<Region prefHeight="92.0" prefWidth="502.0"
					HBox.hgrow="ALWAYS" />
				<Button mnemonicParsing="false" prefHeight="40.0"
					prefWidth="104.0" text="Controles">
					<HBox.margin>
						<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
					</HBox.margin>
				</Button>
			</children>
		</HBox>
	</top>
	<bottom>
		<HBox alignment="CENTER" fillHeight="false" spacing="5.0"
			BorderPane.alignment="TOP_CENTER">
			<children>
				<Button mnemonicParsing="false" onAction="#onStopAction"
					prefHeight="30.0" prefWidth="79.0" text="Stop" />
				<Button mnemonicParsing="false" onAction="#onPlayAction"
					prefHeight="30.0" prefWidth="79.0" text="Play" />
			</children>
			<padding>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</padding>
		</HBox>
	</bottom>
	<padding>
		<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
	</padding>
</BorderPane>
